#pragma once

#include <ostream>
#include_next <vector>

#include <elle/print-fwd.hh>

namespace std
{
  template <typename... Args>
  ostream&
  operator <<(ostream& out, vector<Args...> const& s)
  {
    auto const format = is_fixed(out) ? "%s%f" : "%s%s";
    out << '[';
    auto* sep = "";
    for (auto const& e: s)
    {
      elle::print(out, format, sep, e);
      sep = ", ";
    }
    out << ']';
    return out;
  }
}

// Local Variables:
// mode: c++
// End:
